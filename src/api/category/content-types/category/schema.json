{
  "kind": "collectionType",
  "collectionName": "categories",
  "info": {
    "singularName": "category",
    "pluralName": "categories",
    "displayName": "Categorias",
    "description": "Organize your content into categories"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "text",
      "required": true
    },
    "imagen": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "noticias": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::new.new",
      "inversedBy": "categorias"
    },
    "slug": {
      "type": "uid",
      "targetField": "name",
      "required": true
    }
  }
}
