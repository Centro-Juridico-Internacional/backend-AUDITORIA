{
  "kind": "collectionType",
  "collectionName": "news",
  "info": {
    "singularName": "new",
    "pluralName": "news",
    "displayName": "Noticias"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "titulo": {
      "type": "string",
      "minLength": 5,
      "required": true
    },
    "contenido": {
      "type": "blocks",
      "required": true
    },
    "autor": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::author.author",
      "inversedBy": "noticias"
    },
    "categorias": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "mappedBy": "noticias"
    },
    "imagenes": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "UrlYoutube": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "titulo",
      "required": true
    }
  }
}
